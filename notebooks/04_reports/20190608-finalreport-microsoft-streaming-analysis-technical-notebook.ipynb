    "    if amount.startswith('$'):\n",
    "        amount = amount[1:]\n",
    "    else:\n",
    "        print('Crap, one of my values didn\\'t start with a dollar sign')\n",
    "    return amount"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "for title in ['production_budget', 'domestic_gross', 'worldwide_gross']:\n",
    "    tn_movie_budgets[title] = tn_movie_budgets[title].map(get_rid_of_dollar_sign)"
   ]
